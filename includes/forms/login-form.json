is_trash":"0","personalData":{"preventIP":false,"retention":{"policy":"delete","retain_entries_days":"7"},"exportingAndErasing":{"enabled":false,"identificationField":"1","columns":{"ip":{"export":false,"erase":false},"source_url":{"export":false,"erase":false},"user_agent":{"export":false,"erase":false}}}},"confirmations":[{"id":"686eb46051b90","name":"Default Confirmation","isDefault":true,"type":"message","message":"<p data-start=\"22\" data-end=\"367\"><strong><span style=\"color: #ff0000;\">Using Advanced Tools for GF plugin to handle redirection after login form submission.<\/span><\/strong><br data-start=\"107\" data-end=\"110\" \/><strong><span style=\"color: #ff0000;\">Redirect URL is taken from <code data-start=\"137\" data-end=\"150\">redirect_to<\/code> query parameter or mapped <code data-start=\"137\" data-end=\"150\">redirect_to<\/code> hidden field;<\/span><\/strong><br data-start=\"190\" data-end=\"193\" \/><strong><span style=\"color: #ff0000;\">falls back to user-specific landing page filter or homepage if none set.<\/span><\/strong><br data-start=\"265\" data-end=\"268\" \/><strong><span style=\"color: #ff0000;\">Users will not see this confirmation message after login because they are redirected automatically.<\/span><\/strong><\/p>\r\n<p data-start=\"369\" data-end=\"405\"><strong data-start=\"369\" data-end=\"380\">Filter:<\/strong> <code data-start=\"381\" data-end=\"405\">gfat_user_landing_page<\/code><\/p>\r\n\r\n<ul data-start=\"407\" data-end=\"634\">\r\n \t<li data-start=\"486\" data-end=\"617\">\r\n<p data-start=\"488\" data-end=\"499\">Parameters:<\/p>\r\n\r\n<ul data-start=\"502\" data-end=\"617\">\r\n \t<li data-start=\"502\" data-end=\"566\">\r\n<p data-start=\"504\" data-end=\"566\"><code data-start=\"504\" data-end=\"518\">$url<\/code> (string) \u2014 Default URL, <code>admin_url()<\/code> if they have access or else <code data-start=\"553\" data-end=\"565\">home_url()<\/code>.<\/p>\r\n<\/li>\r\n \t<li data-start=\"569\" data-end=\"617\">\r\n<p data-start=\"571\" data-end=\"617\"><code data-start=\"571\" data-end=\"578\">$user<\/code> (WP_User) \u2014 The logged-in user object.<\/p>\r\n<\/li>\r\n \t<li><code data-start=\"571\" data-end=\"578\">$access_admin<\/code> (bool) \u2014 Whether the user has access to the admin area.<\/li>\r\n<\/ul>\r\n<\/li>\r\n \t<li data-start=\"618\" data-end=\"634\">\r\n<p data-start=\"620\" data-end=\"634\">Usage example:<\/p>\r\n<\/li>\r\n<\/ul>\r\n<div class=\"contain-inline-size rounded-2xl relative bg-token-sidebar-surface-primary\">\r\n<div class=\"overflow-y-auto p-4\" dir=\"ltr\">\r\n<pre><code>add_filter( 'gfat_user_landing_page', function( $url, $user, $access_admin ) {\r\n    if ( in_array( 'subscriber', (array) $user-&gt;roles ) ) {\r\n        return site_url( '\/subscriber-dashboard\/' );\r\n    }\r\n    return $url;\r\n}, 10, 3 );\r\n<\/code><\/pre>\r\n<\/div>\r\n<\/div>","url":"","pageId":"102","queryString":"","event":"","disableAutoformat":false,"page":"102","conditionalLogic":[]}],"notifications":[]},"version":"2.9.13"}